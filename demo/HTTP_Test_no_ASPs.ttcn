  testcase tc_stress(in integer n) runs on HTTP_client
  {
    var ptcList ptc;
    var integer r;

    for( r := 0; r < n; r := r + 1 ) {
      ptc[r] := HTTP_client.create;
    };

    for( r := 0; r < n; r := r + 1 ) {
      ptc[r].start(singleTC());
    };

    for( r := 0; r < n; r := r + 1 ) {
      ptc[r].done;
    };
  }

